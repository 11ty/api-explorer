<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="description" content="">
		<title>Eleventy API Explorer</title>
		<style>
		body {
			font-family: system-ui, sans-serif;
		}
		input,
		button {
			font-family: inherit;
		}
		input {
			min-width: 25em;
		}
		details {
			margin-bottom: 2em;
		}
		h1 img {
			width: 0.8125em; /* 13px /16 */
			height: 0.8125em; /* 13px /16 */
			border-radius: 0.8125em; /* 13px /16 */
			vertical-align: baseline;
		}
		</style>
	</head>
	<body>
		{% set eleventyLogoUrl %}https://v1.indieweb-avatar.11ty.dev/{{ "https://www.11ty.dev/" | urlencode }}/{% endset %}
		<h1><img src="{{ eleventyLogoUrl }}" alt="Eleventy Logo" width="60" height="60"> Eleventy API Explorer</h1>
		<main>
			{{ content | safe }}
		</main>
	</body>
</html>